    "reusableRule": [
      {
        "name": "rule_dbt_posa_sostituzione_censimento",
        "rule": "mandatory if dbt_tipologia_attivita == 'NUOVA POSA' || dbt_tipologia_attivita == 'SOSTITUZIONE INTERRUTTORE' || dbt_tipologia_attivita == 'CENSIMENTO'#visible if dbt_tipologia_attivita == 'NUOVA POSA' || dbt_tipologia_attivita == 'SOSTITUZIONE INTERRUTTORE' || dbt_tipologia_attivita == 'CENSIMENTO'"
      },
      {
        "name": "rule_dbt_posa_sostituzione_censimento_maniglia_altro",
        "rule": "mandatory if ( dbt_tipo_interruttore == 'FUSIBILI A MANIGLIA' || dbt_tipo_interruttore == 'NON NOTO/ALTRO' ) && ( dbt_tipologia_attivita == 'NUOVA POSA' || dbt_tipologia_attivita == 'SOSTITUZIONE INTERRUTTORE' || dbt_tipologia_attivita == 'CENSIMENTO' )#visible if dbt_tipologia_attivita == 'NUOVA POSA' || dbt_tipologia_attivita == 'SOSTITUZIONE INTERRUTTORE' || dbt_tipologia_attivita == 'CENSIMENTO'"
      },
      {
        "name": "rule_dbt_parma",
        "rule": "mandatory if dg_provincia == 'PARMA' || dg_provincia == ''#visible if dg_provincia == 'PARMA' || dg_provincia == ''"
      },
      {
        "name": "rule_dbt_torino_chiomonte",
        "rule": "mandatory if dg_provincia == 'TORINO' || dg_provincia == 'CHIOMONTE'#visible if dg_provincia == 'TORINO' || dg_provincia == 'CHIOMONTE'"
      }
    ],
