{
  "trasformatori_mt_bt": {
    "title": {
      "value": "Trasformatori MT-BT"
    },
    "reusableRule": [
      {
        "name": "rule_tr_sostituzione_recupero",
        "rule": "mandatory if tr_tipologia_attivita == 'SOSTITUZIONE' || tr_tipologia_attivita == 'RECUPERO'#visible if tr_tipologia_attivita == 'SOSTITUZIONE' || tr_tipologia_attivita == 'RECUPERO'"
      },
      {
        "name": "rule_tr_nuova_posa_sostituzione_elective",
        "rule": "visible if tr_tipologia_attivita == 'SOSTITUZIONE' || tr_tipologia_attivita == 'NUOVA POSA'"
      },
      {
        "name": "rule_tr_nuova_posa_sostituzione",
        "rule": "mandatory if tr_tipologia_attivita == 'SOSTITUZIONE' || tr_tipologia_attivita == 'NUOVA POSA'#visible if tr_tipologia_attivita == 'SOSTITUZIONE' || tr_tipologia_attivita == 'NUOVA POSA'"
      },
      {
        "name": "rule_tr_nuova_posa_sostituzione_si",
        "rule": "mandatory if ( tr_tipologia_attivita == 'SOSTITUZIONE' || tr_tipologia_attivita == 'NUOVA POSA' ) && tr_nuova_apparecchiatura_presenta_etichetta_rossa == 'SI'#visible if ( tr_tipologia_attivita == 'SOSTITUZIONE' || tr_tipologia_attivita == 'NUOVA POSA' )"
      },
      {
        "name": "rule_tr_censimento",
        "rule": "mandatory if tr_tipologia_attivita == 'CENSIMENTO'#visible if tr_tipologia_attivita == 'CENSIMENTO'"
      },
      {
        "name": "rule_tr_censimento_elective",
        "rule": "visible if tr_tipologia_attivita == 'CENSIMENTO'"
      },
      {
        "name": "rule_tr_censimento_targa_si",
        "rule": "mandatory if tr_tipologia_attivita == 'CENSIMENTO' && tr_possibile_rilevare_dati_targa == 'SI'#visible if tr_tipologia_attivita == 'CENSIMENTO'"
      }
    ],
