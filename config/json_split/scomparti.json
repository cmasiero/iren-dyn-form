{
  "scomparti": {
    "title": {
      "value": "Scomparti"
    },
    "reusableRule": [
      {
        "name": "rule_sostituzione_recupero",
        "rule": "mandatory if sc_tipologia_di_attivita == 'SOSTITUZIONE' || sc_tipologia_di_attivita == 'RECUPERO'#visible if sc_tipologia_di_attivita == 'SOSTITUZIONE' || sc_tipologia_di_attivita == 'RECUPERO'"
      },
      {
        "name": "rule_nuovaposa_sostituzione",
        "rule": "mandatory if sc_tipologia_di_attivita == 'SOSTITUZIONE' || sc_tipologia_di_attivita == 'NUOVA POSA'#visible if sc_tipologia_di_attivita == 'SOSTITUZIONE' || sc_tipologia_di_attivita == 'NUOVA POSA'"
      },
      {
        "name": "rule_nuovaposa_sostituzione_rossa",
        "rule": "mandatory if sc_nuova_apparecchiatura_presenta_etichetta_rossa == 'SI' && ( sc_tipologia_di_attivita == 'SOSTITUZIONE' || sc_tipologia_di_attivita == 'NUOVA POSA' )#visible if ( sc_tipologia_di_attivita == 'SOSTITUZIONE' || sc_tipologia_di_attivita == 'NUOVA POSA' )"
      },
      {
        "name": "rule_censimento",
        "rule": "mandatory if sc_tipologia_di_attivita == 'CENSIMENTO'#visible if sc_tipologia_di_attivita == 'CENSIMENTO'"
      },
      {
        "name": "rule_censimento_facoltativo",
        "rule": "visible if sc_tipologia_di_attivita == 'CENSIMENTO'"
      },
      {
        "name": "rule_censimento_normale_ridotto_quadro_compatto",
        "rule": "mandatory if sc_censimento_caratteristica_involucro == 'SCOMPARTO NORMALE' ||  sc_censimento_caratteristica_involucro == 'SCOMPARTO RIDOTTO' || sc_censimento_caratteristica_involucro ==  'QUADRO COMPATTO'#visible if sc_tipologia_di_attivita == 'CENSIMENTO'"
      },
      {
        "name": "rule_censimento_normale_ridotto",
        "rule": "mandatory if sc_censimento_caratteristica_involucro == 'SCOMPARTO NORMALE' ||  sc_censimento_caratteristica_involucro == 'SCOMPARTO RIDOTTO'#visible if sc_tipologia_di_attivita == 'CENSIMENTO'"
      }
    ],
